@Article{brochard00PMA,
  Title                    = {Dislocation nucleation from surface steps: Atomistic simulation in aluminium},
  Author                   = {S. Brochard and P. Beauchamp and J. Grilhe},
  Journal                  = {Philosophical Magazine A},
  Year                     = {2000},
  Number                   = {3},
  Pages                    = {503-524},
  Volume                   = {80},

  Abstract                 = { Abstract The possible role of surface steps in the nucleation of dislocations from a free surface has been studied by means of a static atomistic simulation using a many-body potential for aluminium. The fcc crystal with a {100} free surface containing a monatomic step lying along a (110) dense direction is submitted to an increasing uniaxial stress along a direction belonging to the {100} plane. For a sufficiently high applied stress, well below the theoretical strength. dislocations are nucleated at the step and glide in the {111} planes emerging at the step. The effect of a stress orientation is examined. The type of dislocation formed. that is Shockley partials of 90° and 30° character or perfect dislocations, is rationalized by considering the resolved shear stress in the {111} planes. The plane where glide will occur is favoured well before nucleation; a shear of increasing amplitude and extension is progressively localized on this plane. The role of the stress field due to the step, in the formation of the localized shear, is discussed. },
  Doi                      = {10.1080/01418610008212065},
  Eprint                   = { http://dx.doi.org/10.1080/01418610008212065 
},
  File                     = {brochard00PMA.pdf:Publis - classé/à lire/brochard00PMA.pdf:PDF},
  Keywords                 = {relevant},
  Owner                    = {rbejaud},
  Timestamp                = {2017.02.23},
  Url                      = { http://dx.doi.org/10.1080/01418610008212065 
}
}

@Article{christian51PRS,
  Title                    = {A Theory of the Transformation in Pure Cobalt},
  Author                   = {Christian, J. W.},
  Journal                  = {Proceedings of the Royal Society of London A: Mathematical, Physical and Engineering Sciences},
  Year                     = {1951},
  Number                   = {1084},
  Pages                    = {51--64},
  Volume                   = {206},

  Abstract                 = {The suggestion that the faults in hexagonal cobalt are in thermal equilibrium with the structure and that the phase change in pure cobalt is influenced considerably by surface energy is examined. It is shown that for the pseudo-equilibrium of a number of individual crystals surface energy effects could produce transformation over a temperature range but the other effects of the transformation are not explained. Quantitative results for the number of hexagonal faults show that these cannot be in thermal equilibrium. The transformation in cobalt is martensitic in nature and the mechanism is considered in terms of {\textquoteright}half dislocations{\textquoteright} of the face-centred-cubic (f.c.c.) lattice. It is shown qualitatively that in the case of these dislocations reflexion at a free surface can lead to the production of a twinned structure or, under favourable conditions, to the transformation from f.c.c. to close-packed-hexagonal (c.p.h.). The faults in hexagonal cobalt can be considered to be the result of {\textquoteright}wrong{\textquoteright} reflexions during the transformation. The characteristic features of martensitic transformations can be explained in terms of the dislocation mechanism.},
  Doi                      = {10.1098/rspa.1951.0055},
  Eprint                   = {http://rspa.royalsocietypublishing.org/content/206/1084/51.full.pdf},
  File                     = {christian51PRS.pdf:Publis - classé/à lire/christian51PRS.pdf:PDF},
  ISSN                     = {0080-4630},
  Keywords                 = {qualityAssured},
  Owner                    = {rbejaud},
  Publisher                = {The Royal Society},
  Timestamp                = {2017.02.23},
  Url                      = {http://rspa.royalsocietypublishing.org/content/206/1084/51}
}

@Article{hirel07SM,
  author         = {P. Hirel and S. Brochard and L. Pizzagalli and P. Beauchamp},
  title          = {Effects of temperature and surface step on the incipient plasticity in strained aluminium studied by atomistic simulations},
  journal        = {Scripta Materialia},
  year           = {2007},
  volume         = {57},
  number         = {12},
  pages          = {1141 - 1144},
  issn           = {1359-6462},
  abstract       = {Atomistic simulations using an \{EAM\} potential are carried out to investigate the first stages of plasticity in aluminium slabs, in particular the effect of both temperature and step geometry on the nucleation of dislocations from surface steps. Temperature is shown to significantly reduce the elastic limit, and to activate the nucleation of dislocation half-loops. Twinning occurs by successive nucleations in adjacent glide planes. The presence of a kinked step is shown to have no influence on the nucleation mechanisms.},
