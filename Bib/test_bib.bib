@Article{zheng14AM,
  author        = {S.J. Zheng and J. Wang and J.S. Carpenter and W.M. Mook and P.O. Dickerson and N.A. Mara and I.J. Beyerlein},
  title         = {Plastic instability mechanisms in bimetallic nanolayered composites},
  journal       = {Acta Materialia},
  year          = {2014},
  volume        = {79},
  number        = {0},
  pages         = {282 - 291},
  issn          = {1359-6454},
  __markedentry = {[rbejaud:]},
  abstract      = {Abstract Strain localization is a common deformation-induced instability in many metallic metals. How it happens is related to both microstructure and the way in which plasticity is mediated prior to localization. Both aspects can fundamentally change in a face-centered cubic metal when it becomes nanostructured; the propensity for deformation twinning increases and the behavior is dominated by dislocation?interface interactions. Here we carry out a transmission electron microscopy investigation to elucidate the collaborative role of deformation twinning and dislocation transmission on the onset of strain localization in nanolayered composites. Two material systems are examined, Cu?Ag and Cu?Nb, and for each system, two interface structures are examined, one prone to dislocation transmission and the other not. We show that dislocation transmission favors crystallographic band formation, whereas dislocations that do not transmit cause interface tilting and are associated with (non-crystallographic) shear band formation.},
  doi           = {http://dx.doi.org/10.1016/j.actamat.2014.07.017},
  file          = {zheng14AM.pdf:Publis - classé/à lire/zheng14AM.pdf:PDF},
  keywords      = {Interfaces, Twinning, Shear bands, Nanolayered composites, rank5, relevant},
  owner         = {rbejaud},
  timestamp     = {2015.12.14},
  url           = {http://www.sciencedirect.com/science/article/pii/S1359645414005217},
}

