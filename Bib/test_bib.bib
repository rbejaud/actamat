@Article{li14PML,
  author        = {Li, Ruizhi and Chew, Huck Beng},
  title         = {Deformation twinning and plastic recovery in Cu/Ag nanolayers under uniaxial tensile straining},
  journal       = {Philosophical Magazine Letters},
  year          = {2014},
  volume        = {94},
  number        = {5},
  pages         = {260-268},
  __markedentry = {[rbejaud:]},
  abstract      = {Molecular dynamics simulations of nanoscale multilayered Cu/Ag metals have been conducted under uniaxial tensile straining to elucidate the out-of-plane deformation behaviour relevant to tensile spallation experiments. In nanolayers with pristine architectures, plastic dissipation prior to crack nucleation is suppressed owing to the high hydrostatic tensile stress state. The presence of sources for micro-twinning partials, such as micro-cracks or columnar grain boundaries, result in abundant deformation twin lamellae across multiple Cu/Ag interlayers. Plastic recovery is observed during unloading of the nanolayers.},
  doi           = {10.1080/09500839.2014.893063},
  eprint        = {http://dx.doi.org/10.1080/09500839.2014.893063},
